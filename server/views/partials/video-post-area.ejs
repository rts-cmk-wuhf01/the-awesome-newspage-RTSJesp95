<div class="video-post-area bg-img bg-overlay" style="background-image: url(/img/bg-img/bg1.jpg);">
    <div class="container">
        <div class="row justify-content-center">

            <% if(typeof videos != 'undefined'){ %>
                <% if (videos.length > 0) {%>
                    <% videos.forEach(video =>{ %>
                        <!-- Single Video Post Start -->
                        <div class="col-12 col-sm-6 col-md-4">
                            <div class="single-video-post">
                                <img src="/img/bg-img/<%= video.video_image%>" alt="">
                                <p>Post Date: <br> <%= video.video_postdate %></p>
                                <!-- Video Button -->
                                <div class="videobtn">
                                    <a href="<%= video.video_url %>" class="videoPlayer"><i
                                    class="fa fa-play" aria-hidden="true"></i></a>
                                </div>
                            </div>
                        </div>
                        <!-- Single Video Post End -->
                <% }); %>
                <% } else{%>
                    <div class="col-12 col-sm-6 col-md-4">
                        <div class="single-video-post">
                            <p>Video is missing!</p>
                        </div>
                    </div>
                <% }%>
            <% } %>
            <!-- Single Video Post End -->

        </div>
    </div>
</div>