<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Categories</title>

    <!-- Favicon -->
    <%- include('partials/icon.ejs') %>

    <!-- Core Stylesheet -->
    <%- include('partials/styles.ejs') %>

</head>
<body>
    <!-- ##### Header Area Start ##### -->
    <%- include('partials/header-full.ejs') %>
    <!-- ##### Header Area End ##### -->

    <!-- ##### Hero Area Start ##### -->
    <%- include('partials/hero-area.ejs') %>
    <!-- ##### Hero Area End ##### -->
    <div class="blog-area section-padding-0-80">
        <div class="container">
            <div class="row">
                <div class="col-12 col-lg-8">
                    <div class="blog-posts-area">
    
                        <% if(typeof articles != 'undefined'){ %>
                            <% if (articles.length > 0) {%>
                                <% articles.forEach(article =>{ %>
                                    <!-- Single Featured Post Start -->
                                    <div class="single-blog-post featured-post mb-30">
                                    <div class="post-thumb">
                                        <a href="#"><img src="/img/bg-img/<%= article.article_image %>" alt="Article Image <% article.article_id %>"></a>
                                    </div>
                                    <div class="post-data">
                                        <a href="#" class="post-catagory"><%= article.category_title  %></a>
                                        <a href="#" class="post-title">
                                            <h6><%= article.article_title %></h6>
                                        </a>
                                        <div class="post-meta">
                                            <p class="post-author">By <a href="#"><%= article.author_name %></a></p>
                                            <p class="post-excerp"><%= article.article_text %></p>
                                            <!-- Post Like & Post Comment -->
                                            <div class="d-flex align-items-center">
                                                <a href="#" class="post-like"><img src="/img/core-img/like.png" alt=""> <span><%= article.article_likes %></span></a>
                                                <a href="#" class="post-comment"><img src="/img/core-img/chat.png" alt=""> <span><%= article.article_comments %></span></a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- Single Featured Post End -->
                            <% }); %>
                            <% } else{%>
                                <div class="single-blog-post featured-post mb-30 empty-article">
                                    <div class="post-thumb">
                                        <img src="https://media1.tenor.com/images/fb6d78954f2ab5e5577c48ee5e7fa5ff/tenor.gif?itemid=7347192" alt="GIF">
                                        <p>No Articles currently available for this category ;_;</p>
                                    </div>
                                </div>
                            <% }%>
                        <% } %>
    
                    </div>
    
                    <nav aria-label="Page navigation example">
                        <ul class="pagination mt-50">
                            <li class="page-item active"><a class="page-link" href="#">1</a></li>
                            <li class="page-item"><a class="page-link" href="#">2</a></li>
                            <li class="page-item"><a class="page-link" href="#">3</a></li>
                            <li class="page-item"><a class="page-link" href="#">4</a></li>
                            <li class="page-item"><a class="page-link" href="#">5</a></li>
                            <li class="page-item"><a class="page-link" href="#">...</a></li>
                            <li class="page-item"><a class="page-link" href="#">10</a></li>
                        </ul>
                    </nav>
                </div>
    
                <div class="col-12 col-lg-4">
                    <div class="blog-sidebar-area">
    
                        <!-- Latest Posts Widget -->
                        <div class="latest-posts-widget mb-50">
    
                            <!-- Single Featured Post Start -->
                            <%- include('partials/sub-partials/single-featured-post-5.ejs') %>
                            <!-- Single Featured Post End -->
    
                            <!-- Single Featured Post Start -->
                            <%- include('partials/sub-partials/single-featured-post-6.ejs') %>
                            <!-- Single Featured Post End -->
    
                            <!-- Single Featured Post Start -->
                            <%- include('partials/sub-partials/single-featured-post-7.ejs') %>
                            <!-- Single Featured Post End -->
    
                            <!-- Single Featured Post Start -->
                            <%- include('partials/sub-partials/single-featured-post-8.ejs') %>
                            <!-- Single Featured Post End -->
    
                            <!-- Single Featured Post Start -->
                            <%- include('partials/sub-partials/single-featured-post-9.ejs') %>
                            <!-- Single Featured Post End -->
    
                            <!-- Single Featured Post Start -->
                            <%- include('partials/sub-partials/single-featured-post-10.ejs') %>
                            <!-- Single Featured Post End -->
    
                        </div>
    
                        <!-- Popular News Widget -->
                        <div class="popular-news-widget mb-50">
                            <h3>4 Most Popular News</h3>
    
                            <!-- Single Popular Blog Start -->
                            <%- include('partials/sub-partials/single-popular-blog-1.ejs') %>
                            <!-- Single Popular Blog End -->
    
                            <!-- Single Popular Blog Start -->
                            <%- include('partials/sub-partials/single-popular-blog-2.ejs') %>
                            <!-- Single Popular Blog End -->
    
                            <!-- Single Popular Blog Start -->
                            <%- include('partials/sub-partials/single-popular-blog-3.ejs') %>
                            <!-- Single Popular Blog End -->
    
                            <!-- Single Popular Blog Start -->
                            <%- include('partials/sub-partials/single-popular-blog-4.ejs') %>
                            <!-- Single Popular Blog End -->
                        </div>
    
                        <!-- Newsletter Widget Blog Area Start -->
                        <%- include('partials/sub-partials/newsletter-widget-blog-area.ejs') %>
                        <!-- Newsletter Widget Area End -->
    
                        <!-- Latest Comments Widget -->
                        <div class="latest-comments-widget">
                            <h3>Latest Comments</h3>
    
                            <!-- Single Comment Start -->
                            <%- include('partials/sub-partials/single-comment-1.ejs') %>
                            <!-- Single Comment End -->
    
                            <!-- Single Comment Start -->
                            <%- include('partials/sub-partials/single-comment-2.ejs') %>
                            <!-- Single Comment End -->
    
                            <!-- Single Comment Start -->
                            <%- include('partials/sub-partials/single-comment-3.ejs') %>
                            <!-- Single Comment End -->
    
                            <!-- Single Comment Start -->
                            <%- include('partials/sub-partials/single-comment-4.ejs') %>
                            <!-- Single Comment End -->
    
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Hvis man har musen over en parentes eller krøllet parentes, kan man se hvor dens sætning slutter henner -->
     <!-- ##### Footer Area Start ##### -->
     <%- include('partials/footer-area.ejs') %>
     <!-- ##### Footer Area End ##### -->
 
     <!-- ##### All Javascript Files ##### -->
     <%- include('partials/scripts.ejs') %>
</body>
</html>